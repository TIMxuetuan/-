/* pages/compileMy/compileMy.wxss */
.compileMy-top {
  height: 150rpx;
  border-bottom: 1px solid #f6f6f6;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 42rpx;
  background-color: #ffffff;
}

.compileMy-top-text {
  font-size: 32rpx;
  font-weight: 500;
  color: #343434;
}

.compileMy-top-right{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.centerIcon{
  line-height: 100rpx;
}

.topRight-img{
  width: 90rpx;
  height: 90rpx;
  margin-right: 22rpx;
  border-radius: 50%;
}
.topRight-icon{
  color: #BCBCBC;
}

.compileMy-two{
  height: 106rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 42rpx;
  background-color: #ffffff;
}

.compileMy-btn{
  margin-top: 112rpx;
  display: flex;
  justify-content: center;
}

.compileMy-btn .van-button{
  width: 468rpx;
  height: 80rpx;
  padding: 0;
  font-size: 28rpx;
  font-weight: 500;
  color: #ffffff;
  line-height: 80rpx;
}

/* 暗黑模式下应用的样式 */
@media (prefers-color-scheme: dark) {
  .compileMy-top{
    background-color: #1A2736;
    border-bottom: 1px solid #E8E9EA;
  }

  .compileMy-two{
    background-color: #1A2736;
  }
.compileMy-top-text{
  color: #E8E9EA;
}




}